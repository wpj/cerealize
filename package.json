{
  "name": "cerealize",
  "version": "0.0.1",
  "description": "Serialize CLI arguments",
  "private": true,
  "main": "index.js",
  "module": "index.js",
  "jsnext:main": "index.js",
  "scripts": {
    "build": "npm run build:cjs",
    "build:cjs": "cross-env BABEL_ENV=cjs babel-node internal/scripts/build-modules.js",
    "clean": "rimraf lib",
    "lint": "npm run lint:js",
    "lint:eslint": "eslint --ignore-path .gitignore --ignore-pattern config --ext .jsx --ext .js/",
    "lint:js": "npm run lint:eslint -- .",
    "precommit": "lint-staged",
    "test": "cross-env BABEL_ENV=test jest",
    "test:watch": "cross-env BABEL_ENV=test jest --watch"
  },
  "author": "Wyatt Johnston <wyjohnston@gmail.com>",
  "repository": "wpj/cerealize",
  "license": "MIT",
  "lint-staged": {
    "**/*.{js,jsx}": [
      "prettier --write",
      "lint:eslint",
      "git add"
    ]
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.0.1",
    "babel-jest": "^21.2.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.26.0",
    "change-case": "^3.0.1",
    "cross-env": "^5.0.5",
    "eslint": "^4.8.0",
    "eslint-config-airbnb": "^15.1.0",
    "eslint-config-prettier": "^2.6.0",
    "eslint-plugin-flowtype": "^2.37.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^5.1.1",
    "eslint-plugin-prettier": "^2.3.1",
    "eslint-plugin-react": "^7.4.0",
    "flow-bin": "^0.56.0",
    "husky": "^0.14.3",
    "jest": "^21.2.1",
    "jest-in-case": "^1.0.2",
    "jsdom": "^11.3.0",
    "lint-staged": "^4.2.3",
    "lodash": "^4.17.4",
    "npm-run-all": "^4.1.1",
    "prettier": "^1.7.3",
    "read-pkg-up": "^2.0.0",
    "rimraf": "^2.6.2",
    "rollup": "^0.50.0",
    "rollup-plugin-babel": "^3.0.2",
    "rollup-plugin-commonjs": "^8.2.1",
    "rollup-plugin-filesize": "^1.4.2",
    "rollup-plugin-node-resolve": "^3.0.0",
    "rollup-plugin-replace": "^2.0.0",
    "rollup-plugin-uglify": "^2.0.1",
    "shelljs": "^0.7.8",
    "write-pkg": "^3.1.0"
  }
}
